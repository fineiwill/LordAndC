$(document).ready(function(){
  if ($(".owl-carousel").length) {
    $(".owl-carousel").owlCarousel({
      "autoplay": true,
      "loop": true,
      "autoplayTimeout": 3000,
      "autoplayHoverPause": true,
      "items": 10,
      "responsive": {
        0: {
          "items": 3,
        },
        480: {
          "items": 4,
        },
        1000: {
          "items": 5,
        },
        1100: {
          "items": 6,
        },
        1400: {
          "items": 8,
        },
        1600: {
          "items": 10,
        },
      }
    });
  }
});